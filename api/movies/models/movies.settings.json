{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "name": "Movies"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "runtime": {
      "type": "string",
      "required": true
    },
    "relaseyear": {
      "type": "string",
      "required": true
    },
    "movietype": {
      "type": "string"
    },
    "info": {
      "type": "component",
      "repeatable": true,
      "component": "movie-info.info"
    },
    "likecount": {
      "type": "integer"
    },
    "reviews": {
      "type": "component",
      "repeatable": true,
      "component": "reviews.reviews"
    },
    "score": {
      "type": "integer"
    },
    "washelpful": {
      "type": "integer"
    }
  }
}
